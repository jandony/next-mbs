{
    "plugins": [
        "prettier-plugin-tailwindcss"
    ],
    "tailwindConfig": "./tailwind.config.ts",
    "tabWidth": 2,
    "useTabs": false,
    "semi": true,
    "singleQuote": true,
    "jsxSingleQuote": false,
    "trailingComma": "es5",
    "bracketSpacing": true,
    "arrowParens": "always",
    "proseWrap": "preserve",
    "endOfLine": "lf"
}